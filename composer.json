{
  "name": "iserv/zeit",
  "description": "An all purpose library for date and time handling.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "psr",
    "psr-20",
    "date",
    "time",
    "clock"
  ],
  "require": {
    "php": ">=8.1",
    "psr/clock": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "IServ\\Library\\Zeit\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "cotor": {
      "friendsofphp/php-cs-fixer": "^3.8",
      "phpstan/phpstan": "^1.7",
      "phpunit/phpunit": "^9.5",
      "vimeo/psalm": "^4.23"
    }
  }
}
